function showHotMessage(item){$.ajax({url:"http://"+HJBase.getCommonHost()+"/handler/hotmessage.json?file="+item.FileName,dataType:"jsonp",jsonp:"callback",jsonpCallback:"getHotMessage",success:function(data){if(data==""){return;}
var referrer=document.referrer.toLowerCase();if($("#hotMessage").length==0){$("#wrapper").prepend("<div id='hotMessage' class='top_warning' style='display: block;'></div>");$("#hjpassport .soundimg").click(function(){if($("#hotMessage").is(":hidden")){$("#hotMessage").slideDown();$("#hjpassport .soundimg").addClass("soundimg_bg2");}
else{$("#hotMessage").slideUp();$("#hjpassport .soundimg").removeClass("soundimg_bg2");}
$("#hjpassport .soundimg").css("cursor","pointer");});}
$("#hotMessage").html(data);$("#hotMessage a:gt(0)").each(function(){$(this).click(function(){DoRecord(this,"热点信息推送_"+item.Name+"_"+$(this).html());})});$("#hotMessage").slideDown();$("#sound a").addClass("soundimg_bg2");$("#sound a").click(function(){if(!$(this).hasClass("soundimg_bg2")){DoRecord(this,"热点信息推送_折叠");}});var record="热点信息推送_"+item.Name+"_展示成功";record+=item.Type=="Search"?"_来自"+getSearchEngineName(referrer)+"_关键词:"+item.Keyword:"";DoRecord(this,record);}});}
function checkHotMessageVisible(){var hideCount=parseInt(Cookie.get("hideHotMessage"));if(hideCount<=0||isNaN(hideCount)){return true;}
Cookie.set("hideHotMessage",(hideCount-1).toString(),60*60*24,"/");return false;}
function closeHotMessage(){$("#hotMessage").slideUp();$("#sound a").removeClass("soundimg_bg2");DoRecord(this,"热点信息推送_关闭");Cookie.set("hideHotMessage","5",60*60*24,"/");}
function checkSite(item){var host=window.location.host;if(item!=null){var site=item.Site.replace(/，/,",").replace(/\s+/,"").split(",");for(var i=0;i<site.length;i++){if(site[i]=="all"){return true;}else if(site[i]=="en"){if(host.indexOf("en.hujiang.com")>-1||host.indexOf("www.hjenglish.com")>-1){return true;}}else if(site[i]=="hj"){if(host.indexOf("www.hujiang.com")>-1){return true;}}else{if(host.indexOf(site[i])>-1){return true;}}}}
return false;}
function matchValidItem(items){var item=null;for(var i=0;i<items.length;i++){switch(items[i].Type){case"Search":item=fromSearchEngine(items[i]);break;case"Referrer":item=checkReferrer(items[i]);break;case"QueryString":item=checkQueryString(items[i]);break;}
if(item!=null){break;}}
if(checkSite(item)){return item;}
return null;}
function fromSearchEngine(item){var referrer=document.referrer.toLowerCase();var keyword=getSearchKeyword(referrer).toUpperCase();var utf8=encodeURIComponent(item.Keyword).toUpperCase();var gb2312=item.GB2312.toUpperCase();if(keyword.indexOf(utf8)>-1){return item;}
if(keyword.indexOf(gb2312)>-1){return item;}
return null;}
function checkReferrer(item){var referrer=document.referrer;if(referrer.indexOf(item.Keyword)>-1){return item;}
return null;}
function checkQueryString(item){var url=window.location.href.toLowerCase();var queryString=url.indexOf("?")>-1?url.split("?")[1]:"";if(queryString.indexOf(item.Keyword)>-1){return item;}
return null;}
function getSearchEngineName(referrer){if(referrer==null||referrer==undefined){return;}
var host=/http:\/\/(.*?)\//.exec(referrer)[0];if(host.indexOf("google")>-1){return"谷歌";}
if(host.indexOf("baidu")>-1){return"百度";}
return host;}
function getSearchKeyword(referrer){if(referrer==""){return"";}
var queryString=referrer.indexOf("?")>-1?referrer.split("?")[1]:"";var keyword="";var host=/http:\/\/(.*?)\//.exec(referrer)[0];if(host.indexOf("baidu")>-1){keyword=new RegExp("(wd|word)=([^&]*)").exec(queryString)[2];}else if(host.indexOf("google")>-1){keyword=new RegExp("q=([^&]*)").exec(queryString)[1];}
return keyword;}
function getHostPrefix(){var prefix="";if(window.location.host.indexOf("dev.")>-1){prefix="dev.";}else if(window.location.host.indexOf("yz.")>-1){prefix="yz.";}
return prefix;}
