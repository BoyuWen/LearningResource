return paramsArr.join('&');}
function getData(url,container){$.ajax({url:url,dataType:'jsonp',success:function(data){successHandler(data,container);},error:errorHandler});}
function successHandler(data,container){if(data.status==0){var courseTable=data.data,containerCount=container.length,modelType;if($.isArray(data.data)){for(var i=0;i<courseTable.length;i++){modelType=courseTable[i].modelType.toLowerCase();if(container.is('.pagesoft_placeholder')){var pageAdvId=courseTable[i].pageAdvId;var _container;try{_container=container.filter('[data-pagesoftid='+pageAdvId+']');}catch(e){container.each(function(){if($(this).attr('data-pagesoftid')==pageAdvId){_container=$(this);return false;}});}
renderTempl(modelType,courseTable[i],_container);}else if(containerCount>1){renderTempl(modelType,courseTable[i],container.filter(':first'));}else{renderTempl(modelType,courseTable[i],container);}}}else{modelType=courseTable.modelType.toLowerCase();renderTempl(modelType,courseTable,container);}}}
function renderTempl(modelType,data,container){var $templ,iWidth;if(isMobile&&modelType=='textcourse'){modelType='imagetextcourse';}
if(modelType=='imagetext'){$templ=$(data.contentText);}else{$templ=$(template(modelType,data));}
iWidth=container.width()?container.width():container.parent().width()||$('#gallery').width();if(iWidth<700&&iWidth>580){$templ.addClass('old');}else if(iWidth<580&&iWidth>500){$templ.addClass('slideArticle');}
if(isMobile){$templ.addClass('mobile').find('.price-wrap').hide();}
if(container.is('.pagesoft_placeholder')){container.replaceWith($templ);}else{if(data.delyPosition=='start'){var gallery,paraInfo;if((gallery=$('#gallery')).length){gallery.after($templ);}else if((paraInfo=container.find('.para-info')).length){paraInfo.after($templ);}else{container.prepend($templ);}}else if(data.delyPosition=='end'){container.append($templ);}}
if($templ.is('.templ-list')){var lis=$templ.find('li');lis.each(function(index,elem){var iWidth=$(this).width();var img=$(this).find('img');var src=img.attr('src');var oImage=new Image();oImage.onload=function(){var ratio=this.width/this.height;if(ratio>1.5){img.css({'max-width':'100%',height:Math.round(iWidth/1.5)})}};oImage.src=src;});}
sendEvent(50,'ruanwen_show',{'location':data.delyPosition,'type':modelType});bindEvent($templ,data.delyPosition,modelType);}
function bindEvent(dom,_location,modelType){dom.on('click','a',function(){sendEvent(50,'ruanwen_click',{'location':_location,'type':modelType});});}
function errorHandler(err){console.error('error from ruanwengunali interface '+err);}
function sendEvent(cId,eventId,params){$.extend(params,{'langs':langs,'site':siteName});ht.sendEvent(cId,eventId,params);}
function start(){var aScript=$('script'),hasScript=false;aScript.each(function(index,elem){var src=$(this).attr('src');if(src&&src.indexOf('keywordForLink')>-1){hasScript=true;return false;}});if(!hasScript){init();}}
$(function(){start();});window.initRuanWen=init;}($,window));
