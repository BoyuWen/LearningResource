function uiEffect(){(function($){$.fn.hoverDelay=function(options){var defaults={hoverDuring:200,outDuring:200,hoverEvent:function(){$.noop();},outEvent:function(){$.noop();}};var sets=$.extend(defaults,options||{});var hoverTimer,outTimer;return $(this).each(function(){$(this).hover(function(){clearTimeout(outTimer);hoverTimer=setTimeout(sets.hoverEvent,sets.hoverDuring);},function(){clearTimeout(hoverTimer);outTimer=setTimeout(sets.outEvent,sets.outDuring);});});}})(jQuery);$('#hjpassport .pass_x').each(function(){var t=$(this);t.hoverDelay({hoverEvent:function(){if(t.hasClass("xx")){t.find(".pass_x_t").find("b").addClass("bg2");t.find(".pass_x_t").addClass("hover");t.find(".pass_x_b").show();HJBuloMsg.hideMsg();}
else{t.find(".pass_x_t").find("b").addClass("bg2");t.find(".pass_x_t").addClass("hover");t.find(".pass_x_b").show();}
$(".pass_site a span").each(function(){if($(this).html().indexOf("沪江")<0){$(this).html("沪江"+$(this).html());}});},outEvent:function(){if(t.hasClass("xx")){t.find(".pass_x_t").find("b").removeClass("bg2");t.find(".pass_x_t").removeClass("hover");t.find(".pass_x_b").hide();HJBuloMsg.showMsg();}
else{t.find(".pass_x_t").find("b").removeClass("bg2");t.find(".pass_x_t").removeClass("hover");t.find(".pass_x_b").hide();}}})});$('#hjpassport .dh').mouseenter(function(){if($('#dh_b_con1 h3').length>0){return}else{$('#dh_b_con1').prepend('<div class="db_h_title cf"><h3 class="fl">语言学习</h3><a class="fr dh_con1_more" rel="nofollow" target="_blank" href="http://xyz.hujiang.com/">更多»</a></div>');$('#dh_b_con2').prepend('<div class="db_h_title"><h3>中小学</h3></div>');}});$("#ing_push").mouseenter(function(){return false;});if($("#hotMessage").length>0){$("#hjpassport .soundimg").click(function(){if($("#hotMessage").is(":hidden")){$("#hotMessage").slideDown();$("#hjpassport .soundimg").addClass("soundimg_bg2");}
else{$("#wrapper").animate({"paddingTop":0},300);$("#hotMessage").slideUp();$("#hjpassport .soundimg").removeClass("soundimg_bg2");$("#sound").removeClass("sound_c");}});$("#hotMessage .warning_close").live("click",function(){$("#hotMessage").slideUp();$("#hjpassport .soundimg").removeClass("soundimg_bg2");$("#sound").removeClass("sound_c");$("#wrapper").animate({"paddingTop":0},300);});}}
